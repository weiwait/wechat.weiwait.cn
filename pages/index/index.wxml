<!--pages/index/index.wxml-->

<swiper id="banner" indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{baseDir + item}}" class="slide-image"/>
    </swiper-item>
  </block>
</swiper>